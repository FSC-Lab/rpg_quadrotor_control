{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build RPG Quadrotor Control",
      "type": "shell",
      "command": "catkin",
      "args": [
        "build",
        "rqt_quad_gui",
        "position_controller",
        "polynomial_trajectories",
        "trajectory_generation_helper",
        "autopilot",
        "quad_launch_files",
        "rpg_rotors_interface"
      ],
      "options": {
        "cwd": "${workspaceFolder}/../.."
      },
      "problemMatcher": [],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
